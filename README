<?php
require_once("../src/Sign.php");

$s = new Sign("key");
$data = [
	"name" => "Jon",
	"age" => "32",
	0 => "test"
];
echo $s->generateSign($data)
echo "<br>";
$data = json_encode($data);
echo $s->generateSign($data);
echo "<br>";

//也支持链接调用

$s = new Sign()
echo $s->setKey("key")->generateSign($data);
echo "<br>";